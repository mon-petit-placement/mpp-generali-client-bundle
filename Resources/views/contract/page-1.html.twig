            <span style='color:black;font-size: 15px;font-family: Arial, Helvetica, sans-serif'>SOUSCRIPTEUR(TRICE) / ASSURE(E)</span>
        </div>

        <!-- BLOC DE PARTIE -->
        <div width='90%' style='margin-top:15px;'>
            <table width='100%'>
                <tbody>
                    <tr valign='top'>
                        <td width='50%' style='padding:5px'>
                            <!-- TITRE CATEGORIE -->
                            <span style='color:rgb(0,201,0);font-size:12px;font-family: Arial, Helvetica, sans-serif'>IDENTITE</span>
                            <br>
                            <!-- REPONSES CATEGORIE -->
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>{{ resp.civility }}</span>
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>Nom : {{ resp.lastname }}</span>
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>Pr&#xE9;nom : {{ resp.firstname }}</span>
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>Nom de naissance : {{ resp.birthname }}</span>
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>Adresse : {{ resp.address }}</span>
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>Code postal : {{ resp.zipcode }}</span>
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>Ville : {{ resp.city }}</span>
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>Date de naissance : {{ resp.birthdate }}</span>
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>Code postal de naissance : {{ resp.birthZipcode }}</span>
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>Ville de naissance : {{ resp.birthCity }}</span>
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>Pays de naissance : {{ resp.birthCountry }}</span>
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>Nationalit&#xE9; : {{ resp.nationality }}</span>
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>R&#xE9;sidence fiscale : {{ resp.taxCountry }}</span>
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>T&#xE9;l&#xE9;phone : {{ resp.phoneNumber }}</span>
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>E-mail : {{ resp.email }}</span>
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>Document d'identit&#xE9; : {{ resp.identityDocumentType }}</span>
                            <br>
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>Situation familiale : {{ resp.maritalStatus }} {{ resp.matrimonialRegime }}</span>
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>Nombre de personnes &#xE0; charge : {{ resp.nbChildren }}</span>
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>Capacit&#xE9; juridique : Majeur(e) capable</span>
                            <br>
                            <br><span style='color:black;font-size:10px;font-family: Arial, Helvetica, sans-serif'>Statut professionnel : {{ resp.professionalStatus }}</span>
